+using ComwellSystemAPI.Interfaces;
+using Microsoft.AspNetCore.Mvc;
+using System;
+using System.Threading.Tasks;
+
+namespace ComwellSystemAPI.Controllers
+{
+    [ApiController]
+    [Route("api/[controller]")]
+    public class RapportController : ControllerBase
+    {
+        // Service til at hente rapportingsdata
+        private readonly IGenereRapport _rapportService;
+        // Service til at håndtere autentificering og autorisation
+        private readonly IAuthService _authService;
+
+        public RapportController(IGenereRapport rapportService, IAuthService authService)
+        {
+            _rapportService = rapportService ?? throw new ArgumentNullException(nameof(rapportService));
+            _authService = authService ?? throw new ArgumentNullException(nameof(authService));
+        }
+
+        // Henter praktikperioder for givet år, kun admins kan
+        [HttpGet("praktikperioder/{year}")]
+        public async Task<IActionResult> GetPraktikPerioder(int year)
+        {
+            var role = await _authService.GetCurrentUserRoleAsync();
+            if (role != "Admin")
+            {
+                return Unauthorized("Kun admins kan få adgang til denne rapport.");
+            }
+
+            var perioder = await _rapportService.GetPraktikPerioderAsync(year);
+            return Ok(perioder);
+        }
+
+        // Henter alle delmål for givet år baseret på deres deadline, kun admins kan
+        [HttpGet("delmål/{year}")]
+        public async Task<IActionResult> GetDelmål(int year)
+        {
+            var role = await _authService.GetCurrentUserRoleAsync();
+            if (role != "Admin")
+            {
+                return Unauthorized("Kun admins kan få adgang til denne rapport.");
+            }
+
+            var delmål = await _rapportService.GetDelmålAsync(year);
+            return Ok(delmål);
+        }
+
+        // Henter alle brugere, der startede i givet år, kun admins kan
+        [HttpGet("brugere/{year}")]
+        public async Task<IActionResult> GetBrugere(int year)
+        {
+            var role = await _authService.GetCurrentUserRoleAsync();
+            if (role != "Admin")
+            {
+                return Unauthorized("Kun admins kan få adgang til denne rapport.");
+            }
+
+            var brugere = await _rapportService.GetBrugereAsync(year);
+            return Ok(brugere);
+        }
+
+        // Henter alle delmål for en given måned i et givet år baseret på deres deadline
+        [HttpGet("delmål/måned/{year}/{month}")]
+        public async Task<IActionResult> GetDelmålByMonth(int year, int month)
+        {
+            var role = await _authService.GetCurrentUserRoleAsync();
+            if (role != "Admin")
+            {
+                return Unauthorized("Kun admins kan få adgang til denne rapport.");
+            }
+
+            var delmål = await _rapportService.GetDelmålMånedAsync(year, month);
+            return Ok(delmål);
+        }
+    }
+}

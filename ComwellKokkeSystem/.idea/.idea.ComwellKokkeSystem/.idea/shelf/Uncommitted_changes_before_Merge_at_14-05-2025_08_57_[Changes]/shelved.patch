+@page "/rapportside"
+@using Modeller
+@using ComwellKokkeSystem.Service
+@inject IGenereRapportService RapportService
+@inject IAuthService AuthService
+@inject NavigationManager NavigationManager
+
+<h3>HR Rapport</h3>
+
+if (!isAdmin)
+{
+    <p>Adgang nægtet. Kun admins kan se denne side.</p>
+}
+else if (praktikperioder == null || delmål == null || brugere == null)
+{
+    <p><em>Loading...</em></p>
+}
+else 
+{
+    <h4>Praktikperioder</h4>
+    <select @onchange="ChangeYear">
+        <option value="2025">2025</option>
+        <option value="2026">2026</option>
+        <option value="2027">2027</option>
+    </select>
+    <table class="table">
+        <thead>
+            <tr>
+                <th>Navn</th>
+                <th>Start Dato</th>
+                <th>Slut Dato</th>
+            </tr>
+        </thead>
+        <tbody>
+            @foreach (var periode in praktikperioder)
+            {
+                <tr>
+                    <td>@periode.Navn</td>
+                    <td>@periode.StartDato.ToString("dd/MM/yyyy")</td>
+                    <td>@periode.SlutDato.ToString("dd/MM/yyyy")</td>
+                </tr>
+            }
+        </tbody>
+    </table>
+
+    <h4>Delmål Deadlines</h4>
+    <table class="table">
+        <thead>
+            <tr>
+                <th>Beskrivelse</th>
+                <th>Deadline</th>
+            </tr>
+        </thead>
+        <tbody>
+            @foreach (var delmål in delmål)
+            {
+                <tr>
+                    <td>@delmål.Beskrivelse</td>
+                    <td>@delmål.Deadline.ToString("dd/MM/yyyy")</td>
+                </tr>
+            }
+        </tbody>
+    </table>
+
+    <h4>Nye Brugere</h4>
+    <table class="table">
+        <thead>
+            <tr>
+                <th>Navn</th>
+                <th>Start Dato</th>
+            </tr>
+        </thead>
+        <tbody>
+            @foreach (var bruger in brugere)
+            {
+                <tr>
+                    <td>@bruger.Navn</td>
+                    <td>@bruger.StartDato.ToString("dd/MM/yyyy")</td>
+                </tr>
+            }
+        </tbody>
+    </table>
+}
+
+@code {
+    private List<Praktikperiode> praktikperioder;
+    private List<Modeller.Delmål> delmål;
+    private List<UserModel> brugere;
+    private int selectedYear = 2025;
+    private bool isAdmin = false;
+
+    protected override async Task OnInitializedAsync()
+    {
+        // Tjek om brugeren er admin
+        var role = await AuthService.GetCurrentUserRoleAsync();
+        if (role != "Admin")
+        {
+            isAdmin = false;
+            NavigationManager.NavigateTo("/access-denied");
+            return;
+        }
+        isAdmin = true;
+
+        // Hent data
+        await LoadData();
+    }
+
+    private async Task LoadData()
+    {
+        praktikperioder = await RapportService.GetPraktikPerioderAsync(selectedYear);
+        delmål = await RapportService.GetDelmålAsync(selectedYear);
+        brugere = await RapportService.GetBrugereAsync(selectedYear);
+    }
+
+    private async void ChangeYear(ChangeEventArgs e)
+    {
+        selectedYear = int.Parse(e.Value.ToString());
+        await LoadData();
+        StateHasChanged();
+    }
+}
